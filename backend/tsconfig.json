{
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES6",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  